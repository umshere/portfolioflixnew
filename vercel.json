{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "github": {
        "enabled": true,
        "silent": false
    },
    "build": {
        "env": {
            "NEXT_PUBLIC_SITE_URL": "https://portfolio-next-ahnbr07ix-umsheres-projects.vercel.app"
        }
    }
}